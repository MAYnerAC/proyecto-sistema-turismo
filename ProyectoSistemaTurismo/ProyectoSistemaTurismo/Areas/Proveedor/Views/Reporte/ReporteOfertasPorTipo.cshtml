@model List<ProyectoSistemaTurismo.ReportModels.OfertaPorTipoReporte>

@{
    ViewBag.Title = "Reporte de Ofertas por Tipo";
}

<h2>@ViewBag.Title</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Tipo de Oferta</th>
            <th>Cantidad</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.TipoOferta</td>
                <td>@item.Cantidad</td>
            </tr>
        }
    </tbody>
</table>
